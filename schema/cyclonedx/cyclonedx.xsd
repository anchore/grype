                                        <xs:annotation>
                                            <xs:documentation>
                                                Secure Shell
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="ipsec">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Internet Protocol Security
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="ike">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Internet Key Exchange
                                            </xs:documentation>
                                        </xs:annotation>
