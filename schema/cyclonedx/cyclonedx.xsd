                                <xs:restriction base="xs:integer">
                                    <xs:minInclusive value="0"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="nistQuantumSecurityLevel" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
