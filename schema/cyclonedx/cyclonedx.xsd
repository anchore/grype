                                        </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="cc-eal5+">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Common Criteria - Evaluation Assurance Level 5 (Augmented)
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:enumeration>
                                    <xs:enumeration value="cc-eal6">
                                        <xs:annotation>
                                            <xs:documentation>
